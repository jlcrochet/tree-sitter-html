{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "html",
      "camelcase": "Html",
      "title": "HTML",
      "scope": "source.html",
      "file-types": [
        "html"
      ],
      "injection-regex": "^html$",
      "class-name": "TreeSitterHtml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for HTML",
    "authors": [
      {
        "name": "Jeffrey Crochet"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-html"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}